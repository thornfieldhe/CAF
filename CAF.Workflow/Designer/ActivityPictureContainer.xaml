<UserControl x:Class="Shareidea.Web.UI.Control.Workflow.Designer.ActivityPictureContainer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
 xmlns:pic="clr-namespace:Shareidea.Web.UI.Control.Workflow.Designer.Picture"
 >
         <Grid Name="gridContainer"     Width="200" Height="160"  HorizontalAlignment="Center" VerticalAlignment="Center">
            <pic:BranchActivity x:Name="picBRANCH"></pic:BranchActivity>
        <pic:InteractoinAcitivity x:Name="picINTERACTION"></pic:InteractoinAcitivity>
        <pic:InitialAcitivity    x:Name="picBegin"></pic:InitialAcitivity>
        <pic:CompletionActivity     x:Name="picEnd"></pic:CompletionActivity>
        <pic:AutomationActivity    x:Name="picAUTOMATION"></pic:AutomationActivity>
        <pic:MergeActivity   x:Name="picMERGE"></pic:MergeActivity>
        <TextBlock   x:Name="txtActivityName" Foreground="Black" Text="新建活动"  FontSize="12"    
                     VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap"  MaxWidth="90" >

            </TextBlock>
        </Grid>  
</UserControl>
